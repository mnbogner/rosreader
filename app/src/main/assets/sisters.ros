            <profile id="8273-33c1-fb26-0fdc::bbe9-1d77-5c41-12c3" name="Rites of Fire" page="102" publicationId="d124-b283-pubN67455" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
              <characteristics>
