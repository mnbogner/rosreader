            <profile id="8a2a-e4bb-03f4-1d88::347d-0f86-cf91-8731" name="The Fiery Heart" page="79" publicationId="d124-b283-pubN72234" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
