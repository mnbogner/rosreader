            <profile id="61f5-e29b-7836-672b::8265-15f3-9dbe-99d1" name="Emanatus Force Field" publicationId="ebe8-544e-pubN65537" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
