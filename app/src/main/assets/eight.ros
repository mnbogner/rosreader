                    <profile id="ef69-08cf-a255-2739::cd5d-8c24-d205-fd02::f4d9-8c93-9f6e-55e1::08eb-edc0-eb3e-1aab" name="MV7 Marker Drone" page="109" publicationId="c0bb-c0cd-pubN68738" hidden="false" typeId="800f-21d0-4387-c943" typeName="Unit">
