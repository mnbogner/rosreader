              </characteristics>
            </profile>
            <profile id="b4da-1c17-3d75-4dc9::960e-a363-b050-f8ef::100e-a5f5-4af4-8c40" name="Psyker" hidden="false" typeId="bc97-dea9-9e88-bb7d" typeName="Psyker">
