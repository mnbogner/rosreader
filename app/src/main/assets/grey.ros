    <force id="78a7-9f78-a4c1-ea1c" name="Vanguard Detachment +1CP" entryId="41af-75ce-79d2-ddff" catalogueId="0cc2-3545-6762-a3f7" catalogueRevision="51" catalogueName="Imperium - Grey Knights">
